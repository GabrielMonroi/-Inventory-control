<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.biel.yyy">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.example.biel.yyy.SplashActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.example.biel.yyy.Menu" />
        <activity android:name="com.example.biel.yyy.WebSuport" />
        <activity android:name="com.example.biel.yyy.Login" />
        <activity android:name="com.example.biel.yyy.VisualizaIndividual" />
        <activity android:name="com.example.biel.yyy.UpdateProduto" />
        <activity android:name="com.example.biel.yyy.Produto" />
        <activity android:name="com.example.biel.yyy.MostrarTodosProduto" />
        <activity android:name="com.example.biel.yyy.CadastroProduto"></activity>
        <activity android:name="com.example.biel.yyy.Contato"></activity>
    </application>

</manifest>